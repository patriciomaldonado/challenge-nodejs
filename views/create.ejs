<!DOCTYPE html>
<html>

  <head>
    <%- include('./partials/head.ejs') %>
    <link rel="stylesheet" href="/stylesheets/login.css">
  </head>

    <body>
    <%- include('./partials/nav.ejs') %>
    <div class="contenete">
        <main class="main">
            <div class="container2">
                <section class="wrapper">
                    <div class="heading">
                        <h1 class="text text-large">Create Movie</h1>
                    </div>
                    <form name="signin" class="form">
                        <div class="input-control">
                            <label for="password" class="input-label" hidden>Id</label>
                            <input type="password" name="password" id="password" class="input-field" placeholder="Id">
                        </div>
                        <div class="input-control">
                            <label for="email" class="input-label" hidden>Title</label>
                            <input type="email" name="email" id="email" class="input-field" placeholder="Title">
                        </div>
                        <div class="input-control">
                            <label for="password" class="input-label" hidden>Rating</label>
                            <input type="text" name="rating" id="password" class="input-field" placeholder="Rating">
                        </div>
                        <div class="input-control">
                            <label for="password" class="input-label" hidden>Awards</label>
                            <input type="password" name="password" id="password" class="input-field" placeholder="Awards">
                        </div>
                        <div class="input-control">
                            <label for="password" class="input-label" hidden>Release-date</label>
                            <input type="password" name="password" id="password" class="input-field" placeholder="Release-date">
                        </div>
                        <div class="input-control">
                            <label for="password" class="input-label" hidden>Length</label>
                            <input type="password" name="password" id="password" class="input-field" placeholder="length">
                        </div>
                        <div style="justify-content: center;" class="input-control">
                            <input type="submit" name="submit" class="input-submit" value="Enviar" disabled>
                        </div>
                    </form>
                </section>
            </div>
        </main>
    </div>

  </body>
</html>
